enum Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : String {
    Maybe::Nothing or "Hello"
  }
}
---------------------------------------------------------OperationOrTypeMismatch
enum Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : String {
    Maybe::Just(0) or "Hello"
  }
}
-------------------------------------------------------------OperationOrNotMaybe
enum Maybe(a) {
  Nothing
  Just(a)
}

component Main {
  fun render : String {
    0 or "Hello"
  }
}
